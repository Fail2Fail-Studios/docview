{
  "name": "nuxt-ui-pro-template-docs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "compose:up": "sudo docker compose --profile dev up --build -d",
    "compose:down": "sudo docker compose --profile dev down",
    "compose:logs": "sudo docker compose --profile dev logs -f",
    "compose:exec": "sudo docker compose exec app sh",
    "compose:build": "sudo docker compose --profile dev build",
    "compose:restart": "sudo docker compose --profile dev restart",
    "compose:stop": "sudo docker compose --profile dev stop",
    "compose:prod:up": "sudo docker compose --profile prod up --build -d",
    "compose:prod:down": "sudo docker compose --profile prod down",
    "compose:prod:logs": "sudo docker compose --profile prod logs -f",
    "compose:prod:restart": "sudo docker compose --profile prod restart",
    "compose:prod:stop": "sudo docker compose --profile prod stop",
    "screen:up": "screen -S f2f-docview -d -m pnpm dev",
    "screen:down": "screen -X -S f2f-docview quit",
    "reconnect": "screen -r f2f-docview"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.57",
    "@iconify-json/simple-icons": "^1.2.43",
    "@nuxt/content": "^3.6.3",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^4.0.0",
    "@toast-ui/editor": "^3.2.2",
    "@toast-ui/vue-editor": "^3.2.3",
    "nuxt": "^4.0.0",
    "nuxt-auth-utils": "^0.5.23",
    "nuxt-og-image": "^5.1.9",
    "sass": "^1.90.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.5.2",
    "eslint": "^9.31.0",
    "typescript": "^5.8.3",
    "vue-tsc": "^3.0.1"
  },
  "resolutions": {
    "unimport": "4.1.1"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "vue-demi"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.18.0",
  "version": "0.14.0"
}
