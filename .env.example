# Production license for @nuxt/ui-pro, get one at https://ui.nuxt.com/pro/purchase
NUXT_UI_PRO_LICENSE=

# Public URL, used for OG Image when running nuxt generate
NUXT_PUBLIC_SITE_URL=

# REQUIRED: Session password for encrypting cookies (minimum 32 characters)
# Generate a secure password: openssl rand -base64 32
NUXT_SESSION_PASSWORD=

# Discord OAuth Configuration
NUXT_OAUTH_DISCORD_CLIENT_ID=
NUXT_OAUTH_DISCORD_CLIENT_SECRET=

# Base URL for authentication callbacks (e.g., http://localhost:3000 or https://yourdomain.com)
NUXT_AUTH_BASE_URL=

# Optional: Custom redirect URL (defaults to NUXT_AUTH_BASE_URL/auth/discord)
NUXT_OAUTH_DISCORD_REDIRECT_URL=

# Document Sync Configuration
# Source directory containing the content to sync FROM
NUXT_SYNC_SOURCE_PATH=
# Destination directory to sync TO (usually the content folder of this project)
NUXT_SYNC_DESTINATION_PATH=
# Optional: Sync timeout in milliseconds (default: 30000)
NUXT_SYNC_TIMEOUT=30000

# Git Repository Configuration
# Local path to the git repository (f2f-una directory)
NUXT_GIT_REPO_PATH=
# Git repository URL (default: https://github.com/Fail2Fail-Studios/una)
NUXT_GIT_REPO_URL=https://github.com/Fail2Fail-Studios/una
# Git branch to pull from (default: main)
NUXT_GIT_BRANCH=main
# Optional: Git operation timeout in milliseconds (default: 60000)
NUXT_GIT_TIMEOUT=60000

# Git Authentication (for private repositories or when git config is not available)
# GitHub username or organization name
NUXT_GIT_USERNAME=
# GitHub Personal Access Token with repo permissions
# Generate at: https://github.com/settings/tokens
NUXT_GIT_TOKEN=
